    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "browserify": "^13.1.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
